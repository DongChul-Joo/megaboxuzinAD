<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cinema">

<insert id="insertCinema" parameterType="com.zinsupark.cinema.Cinema">
	insert into cinema(cmCode,cmName,cmLocation,cmRange,cmSeatTot,cmSeatMap) values
				(cinema_seq.nextval,#{cmName},#{cmLocation},#{cmRange},#{cmSeatTot},#{cmSeatMap})
</insert>
</mapper>